<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Product_Total_Annual_Rate__c</fullName>
    <description>Calculates the total annual rate of a product</description>
    <externalId>false</externalId>
    <formula>IF( End_Date__c = Start_Date__c, 0 , ((UnitPrice /Term_Days__c)*365)* Quantity  )</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Product Total Annual Rate</label>
    <precision>18</precision>
    <required>false</required>
    <scale>2</scale>
    <type>Currency</type>
</CustomField>
